image {
  width: 100%;
  height: 100%;
}
#container {
  min-height: 100vh;
}
#tab {
  min-height: 100vh;
  background-color: #F2F2F2;
}
#tab-nav {
  height: 80rpx;
  position: fixed;
  top: 0;
  background-color: #FFF;
}
#tab-content {
  margin-top: 80rpx;
}
.item {
  margin: 30rpx 25rpx;
  width: fill-available;
  background: white;
  padding: 20rpx;
}
.itemHead {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
}
.itemBody,.itemOther {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.itemOther{
  margin-top: 30rpx;
}
.itemImg {
  width: 113rpx;
  height: 113rpx;
}
.itemImg image {
  border-radius: 50%;
}
.itemMsg {
  margin-left: 25rpx;
}
.itemName {
  font-size: 20px;
  font-weight: 600;
}
.itemLv {
  margin-top: 10rpx;
}
.itemNum {
  color: #9f9f9f;
}
.itemMoney {
  height: 45rpx;
  border-radius: 20rpx;
  padding: 5rpx 15rpx;
  border: 1rpx solid #f13839;
  color: #f13839;
}